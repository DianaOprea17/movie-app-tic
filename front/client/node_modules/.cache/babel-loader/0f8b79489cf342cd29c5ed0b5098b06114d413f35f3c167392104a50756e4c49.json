n      });\r\n\r\n      const data = await response.json();\r\n\r\n      if (response.ok) {\r\n        console.log('Login successful:', data);\r\n        this.$router.push('/home');  // Redirecționează utilizatorul la homepage\r\n      } else {\r\n        this.errorMessage = data.message || 'Login failed';\r\n      }\r\n\r\n    } catch (error) {\r\n      console.error('Error logging in: ', error);\r\n      this.errorMessage = 'Error logging in. Please try again.';\r\n    }\r\n  }\r\n}\r\n\r\n};\r\n</script>\r\n\r\n<style src=\"@/styles/Login.css\"></style>\r\n  "],"mappings":";AAmCA,SAASA,IAAI,QAAO,kBAAkB;AACtC,SAASC,0BAA0B,EAAEC,UAAS,QAAS,eAAe;AAEtE,eAAe;EACbC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IAClB,CAAC;EACD,CAAC;EACDC,OAAO,EAAE;IACT,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF;QACA,MAAMN,0BAA0B,CAACD,IAAI,EAAE,IAAI,CAACI,KAAK,EAAE,IAAI,CAACC,QAAQ,CAAC;QACjEG,OAAO,CAACC,GAAG,CAAC,gBAAgB,CAAC;;QAE7B;QACA;;QAEA;QACA,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,6BAA6B,EAAE;UAC1DC,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACP,cAAc,EAAE,kBAAkB,CAAE;UACtC,CAAC;UACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;YAAEZ,KAAK,EAAE,IAAI,CAACA,KAAK;YAAEC,QAAQ,EAAE,IAAI,CAACA;UAAS,CAAC,CAAC,CAAE;QACxE,CAAC,CAAC;QAEF,MAAMF,IAAG,GAAI,MAAMO,QAAQ,CAACO,IAAI,CAAC,CAAC;QAElC,IAAIP,QAAQ,CAACQ,EAAE,EAAE;UACfV,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEN,IAAI,CAAC;UACtC,IAAI,CAACgB,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC,EAAG;QAC/B,OAAO;UACL,IAAI,CAACC,YAAW,GAAIlB,IAAI,CAACmB,OAAM,IAAK,cAAc;QACpD;MAEF,EAAE,OAAOC,KAAK,EAAE;QACdf,OAAO,CAACe,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;QAC1C,IAAI,CAACF,YAAW,GAAI,qCAAqC;MAC3D;IACF;EACF;AAEA,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}